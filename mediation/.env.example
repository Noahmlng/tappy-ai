# Mediation API (.env.example)
# Scope: prod-only runtime for Runtime API + Control Plane API.

# Required in production
SUPABASE_DB_URL=
MEDIATION_ALLOWED_ORIGINS=https://dashboard.example.com

# Optional provider credentials (empty is allowed; runtime can degrade to no-bid/house ads)
OPENROUTER_API_KEY=
OPENROUTER_MODEL=stepfun/step-3.5-flash:free
CJ_TOKEN=
PARTNERSTACK_API_KEY=

# Test-only (integration/e2e should use a non-production DB)
SUPABASE_DB_URL_TEST=

# Local server entry (tests/devtools only; production deploy uses Vercel handlers)
MEDIATION_ENABLE_LOCAL_SERVER=false
MEDIATION_GATEWAY_HOST=127.0.0.1
MEDIATION_GATEWAY_PORT=3100
