OPENROUTER_API_KEY=
OPENROUTER_MODEL=stepfun/step-3.5-flash:free
CJ_TOKEN=
PARTNERSTACK_API_KEY=
SUPABASE_DB_URL=
HOUSE_ADS_SOURCE=supabase
HOUSE_ADS_DB_CACHE_TTL_MS=15000
HOUSE_ADS_DB_FETCH_LIMIT=1500
REDIS_URL=redis://127.0.0.1:6379/0
NATS_URL=nats://127.0.0.1:4222
SYNADIA_CLOUD_USER_PUBLIC_KEY=
SYNADIA_CLOUD_USER_SEED=
# Alternative auth material:
# SYNADIA_CLOUD_CREDS=
# SYNADIA_CLOUD_CREDS_FILE=
DOPPLER_TOKEN=
DOPPLER_PROJECT=
DOPPLER_CONFIG=
GRAFANA_URL=
GRAFANA_SERVICE_ACCOUNT_TOKEN=
SERVICE_TOKEN_ACTIVE_KID=k2026q1
SERVICE_TOKEN_KEYS_JSON={"k2026q1":"replace-with-strong-secret","k2025q4":"replace-with-previous-secret"}
SIMULATOR_GATEWAY_PORT=3100
SIMULATOR_GATEWAY_HOST=127.0.0.1
SIMULATOR_DASHBOARD_AUTH_REQUIRED=true
SIMULATOR_RUNTIME_AUTH_REQUIRED=true
# Strict manual onboarding mode:
# - true: do not auto-provision bootstrap API key; runtime calls must use keys issued from dashboard/API.
# - false: keep local bootstrap convenience behavior.
SIMULATOR_STRICT_MANUAL_INTEGRATION=false
# Persist settlement + runtime logs into Supabase when DB URL is configured.
# Allowed: auto | supabase | state_file
# Legacy compatibility: postgres will be treated as supabase with a warning.
SIMULATOR_SETTLEMENT_STORAGE=auto
# Fail fast when Supabase persistence is unavailable.
SIMULATOR_REQUIRE_DURABLE_SETTLEMENT=false
SIMULATOR_REQUIRE_RUNTIME_LOG_DB_PERSISTENCE=false
# Stable bootstrap API key injected into simulator control-plane state on startup/reset.
SIMULATOR_BOOTSTRAP_API_KEY=sk_staging_simulator_local_bootstrap_v1
